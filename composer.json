{
    "name": "bellows-app/plugin-inertia-server-side-rendering",
    "description": "Bellows plugin for integrating Inertia SSR into your app.",
    "keywords": [
        "laravel",
        "console",
        "cli",
        "forge",
        "tools",
        "productivity"
    ],
    "homepage": "https://bellows.dev",
    "type": "bellows-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Joe Tannenbaum",
            "email": "joe@joe.codes"
        }
    ],
    "require": {
        "php": "^8.0",
        "bellows-app/plugin-sdk": "^0.1.0"
    },
    "require-dev": {
        "illuminate/http": "^10.0",
        "orchestra/testbench": "^8.5",
        "pestphp/pest": "^2.0",
        "spatie/laravel-ray": "^1.32",
        "tightenco/duster": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Bellows\\Plugins\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
